#: src/bot/bot.py:211 src/bot/bot.py:297 src/bot/utils/keyboards.py:8
msgid "Yes"
msgstr "Да"

#: src/bot/bot.py:215 src/bot/bot.py:302 src/bot/utils/keyboards.py:8
msgid "No"
msgstr "Нет"

#: src/bot/bot.py:466 src/bot/bot.py:493 src/bot/utils/keyboards.py:8
